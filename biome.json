{
	"$schema": "https://biomejs.dev/schemas/1.6.2/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"suspicious": {
				"noExplicitAny": "off",
				"noDebugger": "off",
				"noConsoleLog": "off",
				"noConfusingVoidType": "off"
			},
			"style": {
				"noNonNullAssertion": "off"
			}
		}
	}
}
